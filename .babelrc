{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "useBuiltIns": false,
          "targets": "last 15 versions"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@modules": "./modules",
          "@components": "./components",
          "@hooks": "./hooks",
          "@utils": "./utils",
          "@model": "./model",
          "@HOC": "./HOC"
        }
      }
    ]
  ]
}
