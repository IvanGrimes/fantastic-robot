{
  "version": 2,
  "regions": ["bru"],
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "env": {
    "API_ENDPOINT": "@api_endpoint",
    "API_AUTH_USER": "@api_auth_user",
    "API_AUTH_PASSWORD": "@api_auth_password"
  },
  "build": {
    "env": {
      "MAPS_API_TOKEN": "@maps_api_token",
      "API_ENDPOINT": "@api_endpoint",
      "API_AUTH_USER": "@api_auth_user",
      "API_AUTH_PASSWORD": "@api_auth_password"
    }
  },
  "routes": [
    {
      "src": "^/sitemap.xml$",
      "methods": ["GET"],
      "dest": "/src/lambdas/sitemap.ts"
    },
    {
      "src": "/service-worker.js",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public,max-age=31536000,s-max-age=31536000,immutable",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "(/_next/static/*/.+)",
      "headers": {
        "cache-control": "public,max-age=31536000,s-max-age=31536000,immutable"
      }
    },
    {
      "src": "(/static/*/.+)",
      "headers": {
        "cache-control": "public,max-age=31536000,s-max-age=31536000,immutable"
      }
    },
    {
      "src": "/(((?!\\?|#|\\.|_next|proxy).)*((?!\\?|/|#|\\.).))/",
      "status": 301,
      "headers": { "Location": "/$1" }
    }
  ]
}
