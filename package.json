{
  "name": "photostudio-new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server/index.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node dist/index.js",
    "rendertron": "rendertron",
    "test": "API_ENDPOINT=https://devstudionica.ru/api/ jest",
    "test:watch": "npm test -- --watch",
    "lint": "eslint ./**/*.{ts,tsx} -c .eslintrc",
    "lint:write": "eslint ./**/*.{ts,tsx} -c .eslintrc --fix",
    "prettier": "prettier ./**/*.{ts,tsx,json} --write",
    "analyze": "cross-env NODE_ENV=production BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.54",
    "@types/styled-components": "^5.1.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "jest-next-dynamic": "^1.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "next": "9.3.6",
    "next-images": "^1.4.0",
    "next-offline": "^5.0.2",
    "next-redux-wrapper": "^6.0.2",
    "qs": "^6.9.4",
    "ramda": "^0.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-infinite-scroller": "^1.2.4",
    "react-redux": "^7.2.0",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.2.0",
    "rendertron-middleware": "^0.1.5",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.5",
    "styled-components": "^5.1.1",
    "ts-jest": "^26.1.0",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/qs": "^6.9.2",
    "@types/ramda": "^0.27.6",
    "@types/react": "^16.9.35",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-window": "^1.8.2",
    "@types/redux-devtools-extension": "^2.13.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint:write",
      "git add"
    ]
  }
}
